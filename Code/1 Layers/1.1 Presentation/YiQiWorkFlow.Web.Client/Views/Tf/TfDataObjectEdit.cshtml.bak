@model YiQiWorkFlow.Domain.Tf.TfDataObject
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>通讯配置表表名编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "通讯配置表表名"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/13 20:26:57";var id="@Model.Id";pageName="TfDataObjectEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Tf/SaveTfDataObject",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_TfDataObject .content").first().text(r.Message);
                        $("#Tooltip_Critical_TfDataObject").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_TfDataObject .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_TfDataObject").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            通讯配置表表名编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_TfDataObject">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于通讯配置表表名编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_TfDataObject">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于通讯配置表表名编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_TfDataObject">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于通讯配置表表名编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_TfDataObject">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于通讯配置表表名编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_ObjectId">对象编号:</label>
                <input type="text"  value="@Model.ObjectId" class="mini-textbox" name="ObjectId" id="Control_ObjectId" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_ObjectName">对象名称:</label>
                <input type="text"  value="@Model.ObjectName" class="mini-textbox" name="ObjectName" id="Control_ObjectName" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_ObjectMain">对象主表:</label>
                <input type="text"  value="@Model.ObjectMain" class="mini-textbox" name="ObjectMain" id="Control_ObjectMain" vtype="maxLength:100" />
            </div>

            <div class="ablock">
                <label for="Control_ObjectMaincolumn">主表主键:</label>
                <input type="text"  value="@Model.ObjectMaincolumn" class="mini-textbox" name="ObjectMaincolumn" id="Control_ObjectMaincolumn" vtype="maxLength:100" />
            </div>

            <div class="ablock">
                <label for="Control_ObjectForeigncolumn">主表外键:</label>
                <input type="text"  value="@Model.ObjectForeigncolumn" class="mini-textbox" name="ObjectForeigncolumn" id="Control_ObjectForeigncolumn" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_ObjectIsinsert">是否插入:</label>
                <div id="Control_ObjectIsinsert" name="ObjectIsinsert" class="mini-checkbox" checked="true" readOnly="false" text="是否插入"></div>
            </div>

            <div class="ablock">
                <label for="Control_ObjectIsdelete">是否删除:</label>
                <div id="Control_ObjectIsdelete" name="ObjectIsdelete" class="mini-checkbox" checked="true" readOnly="false" text="是否删除"></div>
            </div>

            <div class="ablock">
                <label for="Control_ObjectIsupdate">是否更新:</label>
                <div id="Control_ObjectIsupdate" name="ObjectIsupdate" class="mini-checkbox" checked="true" readOnly="false" text="是否更新"></div>
            </div>

            <div class="ablock">
                <label for="Control_ObjectDump">冗余对象:</label>
                <input type="text"  value="@Model.ObjectDump" class="mini-textbox" name="ObjectDump" id="Control_ObjectDump" vtype="maxLength:100" />
            </div>

            <div class="ablock">
                <label for="Control_ObjectKeycolumn">冗余对象主掀:</label>
                <input type="text"  value="@Model.ObjectKeycolumn" class="mini-textbox" name="ObjectKeycolumn" id="Control_ObjectKeycolumn" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_ObjectBranchcodecolumn">冗余分店字段:</label>
                <input type="text"  value="@Model.ObjectBranchcodecolumn" class="mini-textbox" name="ObjectBranchcodecolumn" id="Control_ObjectBranchcodecolumn" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_ObjectIsdeletedump">是否删除冗余:</label>
                <div id="Control_ObjectIsdeletedump" name="ObjectIsdeletedump" class="mini-checkbox" checked="true" readOnly="false" text="是否删除冗余"></div>
            </div>

            <div class="ablock">
                <label for="Control_ObjectDescription">传输对象描述:</label>
                <input type="text"  value="@Model.ObjectDescription" class="mini-textbox" name="ObjectDescription" id="Control_ObjectDescription" vtype="maxLength:200" />
            </div>

            <div class="ablock">
                <label for="Control_ObjectDirection">传输方向:</label>
                <input type="text"  value="@Model.ObjectDirection" class="mini-textbox" name="ObjectDirection" id="Control_ObjectDirection" vtype="maxLength:36" />
            </div>

            <div class="ablock">
                <label for="Control_ObjectIstransfer">是否传输:</label>
                <div id="Control_ObjectIstransfer" name="ObjectIstransfer" class="mini-checkbox" checked="true" readOnly="false" text="是否传输"></div>
            </div>

            <div class="ablock">
                <label for="Control_ObjectCondition">传输条件:</label>
                <input type="text"  value="@Model.ObjectCondition" class="mini-textbox" name="ObjectCondition" id="Control_ObjectCondition" vtype="maxLength:200" />
            </div>

            <div class="ablock">
                <label for="Control_ObjectOrder">传输顺序:</label>
                <input type="text" id="Control_ObjectOrder" name="ObjectOrder"  value="@Model.ObjectOrder" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="0"/>
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_Id">:</label>
                <input id="Control_Id" value="@Model.Id" name="Id" class="mini-hidden"/>
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

