@model YiQiWorkFlow.Domain.Rt.RtRetailLog
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>收银日志编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "收银日志"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/15 19:58:57";var id="@Model.Id";pageName="RtRetailLogEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Rt/SaveRtRetailLog",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_RtRetailLog .content").first().text(r.Message);
                        $("#Tooltip_Critical_RtRetailLog").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_RtRetailLog .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_RtRetailLog").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            收银日志编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_RtRetailLog">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于收银日志编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_RtRetailLog">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于收银日志编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_RtRetailLog">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于收银日志编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_RtRetailLog">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于收银日志编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock" style="display:none">
                <label for="Control_SaFlowNumber">流水号:</label>
                <input id="Control_SaFlowNumber" value="@Model.Id" name="Id" class="mini-hidden"/>
            </div>

            <div class="ablock">
                <label for="Control_Cashier">收银员:</label>
                <input type="text"  value="@Model.Cashier" class="mini-textbox" name="Cashier" id="Control_Cashier" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_EmName">收银员姓名:</label>
                <input type="text"  value="@Model.EmName" class="mini-textbox" name="EmName" id="Control_EmName" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_OperatType">操作方式:</label>
                <input type="text"  value="@Model.OperatType" class="mini-textbox" name="OperatType" id="Control_OperatType" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_RtDate">操作时间:</label>
                <input type="text" id="Control_RtDate" name="RtDate"  value="@Model.RtDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsCode">商品编码:</label>
                <input type="text"  value="@Model.GoodsCode" class="mini-textbox" name="GoodsCode" id="Control_GoodsCode" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsBarCode">商品销售码:</label>
                <input type="text"  value="@Model.GoodsBarCode" class="mini-textbox" name="GoodsBarCode" id="Control_GoodsBarCode" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsBarName">销售名称:</label>
                <input type="text"  value="@Model.GoodsBarName" class="mini-textbox" name="GoodsBarName" id="Control_GoodsBarName" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_RealPrice">销售单价:</label>
                <input type="text" id="Control_RealPrice" name="RealPrice"  value="@Model.RealPrice" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_SaleQty">数量:</label>
                <input type="text" id="Control_SaleQty" name="SaleQty"  value="@Model.SaleQty" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_SaleMoney">销售金额:</label>
                <input type="text" id="Control_SaleMoney" name="SaleMoney"  value="@Model.SaleMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_DiscountMoney">折扣金额:</label>
                <input type="text" id="Control_DiscountMoney" name="DiscountMoney"  value="@Model.DiscountMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

