@model YiQiWorkFlow.Domain.Rt.RtPluPromotionGoods
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>单品促销商品明细编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "单品促销商品明细"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/13 20:26:56";var id="@Model.Id";pageName="RtPluPromotionGoodsEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Rt/SaveRtPluPromotionGoods",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_RtPluPromotionGoods .content").first().text(r.Message);
                        $("#Tooltip_Critical_RtPluPromotionGoods").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_RtPluPromotionGoods .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_RtPluPromotionGoods").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            单品促销商品明细编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_RtPluPromotionGoods">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于单品促销商品明细编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_RtPluPromotionGoods">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于单品促销商品明细编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_RtPluPromotionGoods">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于单品促销商品明细编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_RtPluPromotionGoods">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于单品促销商品明细编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_SpNumber">促销单号:</label>
                <input type="text"  value="@Model.SpNumber" class="mini-textbox" name="SpNumber" id="Control_SpNumber" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_SpBeginDate">促销开始日期:</label>
                <input type="text"  value="@Model.SpBeginDate" class="mini-textbox" name="SpBeginDate" id="Control_SpBeginDate" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_SpEndDate">促销终止日期:</label>
                <input type="text"  value="@Model.SpEndDate" class="mini-textbox" name="SpEndDate" id="Control_SpEndDate" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_SpBeginTime">促销开始时间:</label>
                <input type="text"  value="@Model.SpBeginTime" class="mini-textbox" name="SpBeginTime" id="Control_SpBeginTime" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_SpEndTime">促销终止时间:</label>
                <input type="text"  value="@Model.SpEndTime" class="mini-textbox" name="SpEndTime" id="Control_SpEndTime" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_SpW1">星期1:</label>
                <input type="text"  value="@Model.SpW1" class="mini-textbox" name="SpW1" id="Control_SpW1" vtype="maxLength:2" />
            </div>

            <div class="ablock">
                <label for="Control_SpW2">星期2:</label>
                <input type="text"  value="@Model.SpW2" class="mini-textbox" name="SpW2" id="Control_SpW2" vtype="maxLength:2" />
            </div>

            <div class="ablock">
                <label for="Control_SpW3">星期3:</label>
                <input type="text"  value="@Model.SpW3" class="mini-textbox" name="SpW3" id="Control_SpW3" vtype="maxLength:2" />
            </div>

            <div class="ablock">
                <label for="Control_SpW4">星期4:</label>
                <input type="text"  value="@Model.SpW4" class="mini-textbox" name="SpW4" id="Control_SpW4" vtype="maxLength:2" />
            </div>

            <div class="ablock">
                <label for="Control_SpW5">星期5:</label>
                <input type="text"  value="@Model.SpW5" class="mini-textbox" name="SpW5" id="Control_SpW5" vtype="maxLength:2" />
            </div>

            <div class="ablock">
                <label for="Control_SpW6">星期6:</label>
                <input type="text"  value="@Model.SpW6" class="mini-textbox" name="SpW6" id="Control_SpW6" vtype="maxLength:2" />
            </div>

            <div class="ablock">
                <label for="Control_SpW7">星期7:</label>
                <input type="text"  value="@Model.SpW7" class="mini-textbox" name="SpW7" id="Control_SpW7" vtype="maxLength:2" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsBarCode">商品销售码:</label>
                <input type="text"  value="@Model.GoodsBarCode" class="mini-textbox" name="GoodsBarCode" id="Control_GoodsBarCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsCode">商品编码:</label>
                <input type="text"  value="@Model.GoodsCode" class="mini-textbox" name="GoodsCode" id="Control_GoodsCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_IfSalePrice">是否销售价:</label>
                <div id="Control_IfSalePrice" name="IfSalePrice" value="1" class="mini-checkbox" checked="true" readOnly="false" text="销售价"></div>
            </div>

            <div class="ablock">
                <label for="Control_SalePriceOld">原销售价:</label>
                <input type="text" id="Control_SalePriceOld" name="SalePriceOld"  value="@Model.SalePriceOld" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_SalePriceDiscount">销售价折扣:</label>
                <input type="text" id="Control_SalePriceDiscount" name="SalePriceDiscount"  value="@Model.SalePriceDiscount" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_SalePrice">新销售价:</label>
                <input type="text" id="Control_SalePrice" name="SalePrice"  value="@Model.SalePrice" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_IfLimit">是否限量:</label>
                <div id="Control_IfLimit" name="IfLimit" value="1" class="mini-checkbox" checked="true" readOnly="false" text="限量"></div>
            </div>

            <div class="ablock">
                <label for="Control_TotalLimit">总限量:</label>
                <input type="text" id="Control_TotalLimit" name="TotalLimit"  value="@Model.TotalLimit" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_TotalLimitSale">总已销量:</label>
                <input type="text" id="Control_TotalLimitSale" name="TotalLimitSale"  value="@Model.TotalLimitSale" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_DayLimit">日限量:</label>
                <input type="text" id="Control_DayLimit" name="DayLimit"  value="@Model.DayLimit" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_DayLimitDate">日限日期:</label>
                <input type="text" id="Control_DayLimitDate" name="DayLimitDate"  value="@Model.DayLimitDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_DayLimitSale">日已销量:</label>
                <input type="text" id="Control_DayLimitSale" name="DayLimitSale"  value="@Model.DayLimitSale" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_SheetLimit">单笔限量:</label>
                <input type="text" id="Control_SheetLimit" name="SheetLimit"  value="@Model.SheetLimit" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_IfPool">是否扣率:</label>
                <div id="Control_IfPool" name="IfPool" value="1" class="mini-checkbox" checked="true" readOnly="false" text="扣率"></div>
            </div>

            <div class="ablock">
                <label for="Control_PoolRateOld">原扣率:</label>
                <input type="text" id="Control_PoolRateOld" name="PoolRateOld"  value="@Model.PoolRateOld" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_PoolRate">新扣率:</label>
                <input type="text" id="Control_PoolRate" name="PoolRate"  value="@Model.PoolRate" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_IfVip">是否会员:</label>
                <div id="Control_IfVip" name="IfVip" value="1" class="mini-checkbox" checked="true" readOnly="false" text="会员"></div>
            </div>

            <div class="ablock">
                <label for="Control_VipPriceOld">原会员价:</label>
                <input type="text" id="Control_VipPriceOld" name="VipPriceOld"  value="@Model.VipPriceOld" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_VipPrice">新会员价:</label>
                <input type="text" id="Control_VipPrice" name="VipPrice"  value="@Model.VipPrice" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_VipSheetLimit">会员单笔限量:</label>
                <input type="text" id="Control_VipSheetLimit" name="VipSheetLimit"  value="@Model.VipSheetLimit" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_SysGuid">系统标识:</label>
                <input type="text"  value="@Model.SysGuid" class="mini-textbox" name="SysGuid" id="Control_SysGuid" vtype="maxLength:60" />
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_Id">:</label>
                <input id="Control_Id" value="@Model.Id" name="Id" class="mini-hidden"/>
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

