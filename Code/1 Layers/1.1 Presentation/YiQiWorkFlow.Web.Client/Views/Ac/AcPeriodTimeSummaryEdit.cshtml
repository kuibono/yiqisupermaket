@model YiQiWorkFlow.Domain.Ac.AcPeriodTimeSummary
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>客流销售汇总编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "客流销售汇总"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/15 19:58:32";var id="@Model.Id";pageName="AcPeriodTimeSummaryEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Ac/SaveAcPeriodTimeSummary",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_AcPeriodTimeSummary .content").first().text(r.Message);
                        $("#Tooltip_Critical_AcPeriodTimeSummary").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_AcPeriodTimeSummary .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_AcPeriodTimeSummary").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            客流销售汇总编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_AcPeriodTimeSummary">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于客流销售汇总编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_AcPeriodTimeSummary">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于客流销售汇总编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_AcPeriodTimeSummary">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于客流销售汇总编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_AcPeriodTimeSummary">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于客流销售汇总编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_RtFlowNumber">流水号:</label>
                <input id="Control_RtFlowNumber" value="@Model.Id" name="Id" class="mini-hidden"/>
            </div>

            <div class="ablock">
                <label for="Control_RtDate">销售时间:</label>
                <input type="text"  value="@Model.RtDate" class="mini-textbox" name="RtDate" id="Control_RtDate" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_PeriodTime">时段:</label>
                <input type="text"  value="@Model.PeriodTime" class="mini-textbox" name="PeriodTime" id="Control_PeriodTime" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_WhCode">仓库编码:</label>
                <input type="text"  value="@Model.WhCode" class="mini-textbox" name="WhCode" id="Control_WhCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_bCode">分店编码:</label>
                <input type="text"  value="@Model.bCode" class="mini-textbox" name="bCode" id="Control_bCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_PassengerFlow">客流量:</label>
                <input type="text" id="Control_PassengerFlow" name="PassengerFlow"  value="@Model.PassengerFlow" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_AvgSalePrice">平均客单价:</label>
                <input type="text" id="Control_AvgSalePrice" name="AvgSalePrice"  value="@Model.AvgSalePrice" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_SaleQty">数量:</label>
                <input type="text" id="Control_SaleQty" name="SaleQty"  value="@Model.SaleQty" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_SaleMoney">销售金额:</label>
                <input type="text" id="Control_SaleMoney" name="SaleMoney"  value="@Model.SaleMoney" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="2"/>
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

