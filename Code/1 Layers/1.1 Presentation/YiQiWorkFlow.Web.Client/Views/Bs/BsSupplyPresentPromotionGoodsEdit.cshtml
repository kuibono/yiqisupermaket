@model YiQiWorkFlow.Domain.Bs.BsSupplyPresentPromotionGoods
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>配赠促销商品明细编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "配赠促销商品明细"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/15 19:58:43";var id="@Model.Id";pageName="BsSupplyPresentPromotionGoodsEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Bs/SaveBsSupplyPresentPromotionGoods",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_BsSupplyPresentPromotionGoods .content").first().text(r.Message);
                        $("#Tooltip_Critical_BsSupplyPresentPromotionGoods").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_BsSupplyPresentPromotionGoods .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_BsSupplyPresentPromotionGoods").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            配赠促销商品明细编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_BsSupplyPresentPromotionGoods">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于配赠促销商品明细编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_BsSupplyPresentPromotionGoods">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于配赠促销商品明细编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_BsSupplyPresentPromotionGoods">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于配赠促销商品明细编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_BsSupplyPresentPromotionGoods">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于配赠促销商品明细编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_SpNumber">促销单号:</label>
                <input type="text"  value="@Model.SpNumber" class="mini-textbox" name="SpNumber" id="Control_SpNumber" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_SpBeginDate">促销开始日期:</label>
                <input type="text"  value="@Model.SpBeginDate" class="mini-textbox" name="SpBeginDate" id="Control_SpBeginDate" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_SpEndDate">促销终止日期:</label>
                <input type="text"  value="@Model.SpEndDate" class="mini-textbox" name="SpEndDate" id="Control_SpEndDate" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsBarCode">商品销售码:</label>
                <input type="text"  value="@Model.GoodsBarCode" class="mini-textbox" name="GoodsBarCode" id="Control_GoodsBarCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsCode">商品编码:</label>
                <input type="text"  value="@Model.GoodsCode" class="mini-textbox" name="GoodsCode" id="Control_GoodsCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_SupplyQty">订货数量:</label>
                <input type="text" id="Control_SupplyQty" name="SupplyQty"  value="@Model.SupplyQty" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_SupplyPrice">配送价:</label>
                <input type="text" id="Control_SupplyPrice" name="SupplyPrice"  value="@Model.SupplyPrice" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_GoodsBarCodePresent">送购商品销售码:</label>
                <input type="text"  value="@Model.GoodsBarCodePresent" class="mini-textbox" name="GoodsBarCodePresent" id="Control_GoodsBarCodePresent" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsCodePresent">送购商品编码:</label>
                <input type="text"  value="@Model.GoodsCodePresent" class="mini-textbox" name="GoodsCodePresent" id="Control_GoodsCodePresent" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_PresentQty">送购数量:</label>
                <input type="text" id="Control_PresentQty" name="PresentQty"  value="@Model.PresentQty" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_PresentMoney">送购金额:</label>
                <input type="text" id="Control_PresentMoney" name="PresentMoney"  value="@Model.PresentMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_SysGuid">系统标识:</label>
                <input type="text"  value="@Model.SysGuid" class="mini-textbox" name="SysGuid" id="Control_SysGuid" vtype="maxLength:60" />
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_Id">:</label>
                <input id="Control_Id" value="@Model.Id" name="Id" class="mini-hidden"/>
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

