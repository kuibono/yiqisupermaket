@model YiQiWorkFlow.Domain.Bs.BsSupplyDetail
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>分店订货配送单明细编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "分店订货配送单明细"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/15 19:58:42";var id="@Model.Id";pageName="BsSupplyDetailEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Bs/SaveBsSupplyDetail",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_BsSupplyDetail .content").first().text(r.Message);
                        $("#Tooltip_Critical_BsSupplyDetail").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_BsSupplyDetail .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_BsSupplyDetail").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            分店订货配送单明细编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_BsSupplyDetail">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于分店订货配送单明细编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_BsSupplyDetail">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于分店订货配送单明细编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_BsSupplyDetail">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于分店订货配送单明细编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_BsSupplyDetail">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于分店订货配送单明细编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_SpNumber">配送单号:</label>
                <input type="text"  value="@Model.SpNumber" class="mini-textbox" name="SpNumber" id="Control_SpNumber" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_WhCode">仓库编码:</label>
                <input type="text"  value="@Model.WhCode" class="mini-textbox" name="WhCode" id="Control_WhCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_bCode">分店编码:</label>
                <input type="text"  value="@Model.bCode" class="mini-textbox" name="bCode" id="Control_bCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_SpState">配送状态:</label>
                <input type="text"  value="@Model.SpState" class="mini-textbox" name="SpState" id="Control_SpState" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_WhCodeOut">总部仓库:</label>
                <input type="text"  value="@Model.WhCodeOut" class="mini-textbox" name="WhCodeOut" id="Control_WhCodeOut" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsBarCode">商品销售码:</label>
                <input type="text"  value="@Model.GoodsBarCode" class="mini-textbox" name="GoodsBarCode" id="Control_GoodsBarCode" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsCode">商品编码:</label>
                <input type="text"  value="@Model.GoodsCode" class="mini-textbox" name="GoodsCode" id="Control_GoodsCode" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_Specification">规格:</label>
                <input type="text"  value="@Model.Specification" class="mini-textbox" name="Specification" id="Control_Specification" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_PackUnitCode">包装单位:</label>
                <input type="text"  value="@Model.PackUnitCode" class="mini-textbox" name="PackUnitCode" id="Control_PackUnitCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_PurchasePackQty">整件订货数量:</label>
                <input type="text" id="Control_PurchasePackQty" name="PurchasePackQty"  value="@Model.PurchasePackQty" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_SupplyPackQty">整件配送数量:</label>
                <input type="text" id="Control_SupplyPackQty" name="SupplyPackQty"  value="@Model.SupplyPackQty" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_PutinPackQty">整件实收数量:</label>
                <input type="text" id="Control_PutinPackQty" name="PutinPackQty"  value="@Model.PutinPackQty" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_PackCoef">包装系数:</label>
                <input type="text" id="Control_PackCoef" name="PackCoef"  value="@Model.PackCoef" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_PurchaseQty">订货数量:</label>
                <input type="text" id="Control_PurchaseQty" name="PurchaseQty"  value="@Model.PurchaseQty" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_SupplyQty">配送数量:</label>
                <input type="text" id="Control_SupplyQty" name="SupplyQty"  value="@Model.SupplyQty" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_PutinQty">实收数量:</label>
                <input type="text" id="Control_PutinQty" name="PutinQty"  value="@Model.PutinQty" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_SupplyPrice">配送单价:</label>
                <input type="text" id="Control_SupplyPrice" name="SupplyPrice"  value="@Model.SupplyPrice" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_SupplyMoney">配送金额:</label>
                <input type="text" id="Control_SupplyMoney" name="SupplyMoney"  value="@Model.SupplyMoney" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_DiscountMoney">折扣金额:</label>
                <input type="text" id="Control_DiscountMoney" name="DiscountMoney"  value="@Model.DiscountMoney" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_SysGuid">系统标识:</label>
                <input type="text"  value="@Model.SysGuid" class="mini-textbox" name="SysGuid" id="Control_SysGuid" vtype="maxLength:60" />
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_Id">:</label>
                <input id="Control_Id" value="@Model.Id" name="Id" class="mini-hidden"/>
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

