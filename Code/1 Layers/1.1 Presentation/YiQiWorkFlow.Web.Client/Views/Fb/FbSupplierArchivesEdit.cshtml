@model YiQiWorkFlow.Domain.Fb.FbSupplierArchives
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>供货商档案编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "供货商档案"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/15 19:58:50";var id="@Model.Id";pageName="FbSupplierArchivesEdit"; </script>
	<script type="text/javascript" src="/Scripts/Fb/FbSupplierArchivesEdit.js"></script>
</head>
<body>
    <div class="container">
        <h1>
            供货商档案编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_FbSupplierArchives">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于供货商档案编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_FbSupplierArchives">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于供货商档案编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_FbSupplierArchives">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于供货商档案编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_FbSupplierArchives">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于供货商档案编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_SupCode">供货商编码:</label>
                <input id="Control_SupCode" value="@Model.Id" name="Id" class="mini-textbox" enabled="false"/>
            </div>

            <div class="ablock colspan2">
                <label for="Control_SupName">供货商名称:</label>
                <input type="text"  value="@Model.SupName" class="mini-textbox" name="SupName" id="Control_SupName" vtype="maxLength:100" />
            </div>

            <div class="ablock">
                <label for="Control_PyCode">拼音码:</label>
                <input type="text"  value="@Model.PyCode" class="mini-textbox" name="PyCode" id="Control_PyCode" vtype="maxLength:250" />
            </div>

            <div class="ablock">
                <label for="Control_SupTypeCode">供货商类型:</label>
                <input type="text"  value="@Model.SupTypeCode" class="mini-combobox" name="SupTypeCode" url="/Common/GetComboData?table=fb_pa_sup_type&valueColumn=sup_type_code&textColumn=sup_type_name" id="Control_SupTypeCode" required="true" />
            </div>

            <div class="ablock">
                <label for="Control_Functionary">负责人:</label>
                <input type="text"  value="@Model.Functionary" class="mini-textbox" name="Functionary" id="Control_Functionary" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_FunctionaryPhone">负责人电话:</label>
                <input type="text"  value="@Model.FunctionaryPhone" class="mini-textbox" name="FunctionaryPhone" id="Control_FunctionaryPhone" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_Linkman">联系人:</label>
                <input type="text"  value="@Model.Linkman" class="mini-textbox" name="Linkman" id="Control_Linkman" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_ContactPhone">联系人电话:</label>
                <input type="text"  value="@Model.ContactPhone" class="mini-textbox" name="ContactPhone" id="Control_ContactPhone" vtype="maxLength:20" />
            </div>

            <div class="ablock colspan2">
                <label for="Control_ContactAddress">联系地址:</label>
                <input type="text"  value="@Model.ContactAddress" class="mini-textbox" name="ContactAddress" id="Control_ContactAddress" vtype="maxLength:100" />
            </div>

            <div class="ablock">
                <label for="Control_OfficePhone">办公电话:</label>
                <input type="text"  value="@Model.OfficePhone" class="mini-textbox" name="OfficePhone" id="Control_OfficePhone" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_FaxPhone">传真:</label>
                <input type="text"  value="@Model.FaxPhone" class="mini-textbox" name="FaxPhone" id="Control_FaxPhone" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_eMail">邮箱:</label>
                <input type="text"  value="@Model.eMail" class="mini-textbox" name="eMail" id="Control_eMail" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_Postalcode">邮政编码:</label>
                <input type="text"  value="@Model.Postalcode" class="mini-textbox" name="Postalcode" id="Control_Postalcode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_OpenAccount">开户行:</label>
                <input type="text"  value="@Model.OpenAccount" class="mini-textbox" name="OpenAccount" id="Control_OpenAccount" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_BankAccount">帐号:</label>
                <input type="text"  value="@Model.BankAccount" class="mini-textbox" name="BankAccount" id="Control_BankAccount" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_TaxNumber">税号:</label>
                <input type="text"  value="@Model.TaxNumber" class="mini-textbox" name="TaxNumber" id="Control_TaxNumber" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_OpCode">经营方式:</label>
                <input type="text"  value="@Model.OpCode" class="mini-combobox" name="OpCode" id="Control_OpCode" required="true" data="c_OpCode" showNullItem="true" onvaluechanged="onOpCodeChanged" />
            </div>

            <div class="ablock">
                <label for="Control_StockVoucher">进货凭证:</label>
                <input type="text"  value="@Model.StockVoucher" class="mini-combobox" name="StockVoucher" id="Control_StockVoucher" required="true" data="[{id:'普通发票',text:'普通发票'},{id:'增值税发票',text:'增值税发票'},{id:'农收购发票',text:'农收购发票'},{id:'农粮肉发票',text:'农粮肉发票'}]" valueField="text" showNullItem="true" />
            </div>

            <div class="ablock">
                <label for="Control_InputTax">进项税率:</label>
                <input type="text" id="Control_InputTax" name="InputTax"  value="@Model.InputTax" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_BalanceMode">结算方式:</label>
                <input type="text"  value="@Model.BalanceMode" class="mini-combobox" name="BalanceMode" id="Control_BalanceMode" required="true" data="[{id:'预付货款',text:'预付货款'},{id:'货到付款',text:'货到付款'},{id:'实销实付',text:'实销实付'},{id:'扣点结算',text:'扣点结算'},{ id: '销售返还', text: '销售返还' }]" valueField="text" showNullItem="true" />
            </div>

            <div class="ablock">
                <label for="Control_PayMode">付款方式:</label>
                <input type="text"  value="@Model.PayMode" class="mini-combobox" name="PayMode" id="Control_PayMode" required="true" data="[{id:'现金',text:'现金'},{id:'支票',text:'支票'},{id:'银行转账',text:'银行转账'},{id:'托收承付',text:'托收承付'}]" valueField="text" showNullItem="true" />
            </div>

            <div class="ablock">
                <label for="Control_BalancePeriod">结算账期:</label>
                <input type="text"  value="@Model.BalancePeriod" class="mini-combobox" name="BalancePeriod" id="Control_BalancePeriod" required="true" data="[{id:'月结算',text:'月结算'},{id:'季计算',text:'季计算'},{id:'年结算',text:'年结算'}]" valueField="text" showNullItem="true" />
            
            </div>

            <div class="ablock">
                <label for="Control_BalanceDay">结算日:</label>
                <input type="text" id="Control_BalanceDay" name="BalanceDay"  value="@Model.BalanceDay" class="mini-spinner" minValue="0" maxValue="31" decimalPlaces="0"/>
            </div>

            <div class="ablock">
                <label for="Control_OfferMode">供货方式:</label>
                <input type="text"  value="@Model.OfferMode" class="mini-combobox" name="OfferMode" id="Control_OfferMode" data="c_OfferMode" />
            </div>

            <div class="ablock">
                <label for="Control_DeliveryDays">送货天数:</label>
                <input type="text" id="Control_DeliveryDays" name="DeliveryDays"  value="@Model.DeliveryDays" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_PoolRate">扣率:</label>
                <input type="text" id="Control_PoolRate" name="PoolRate"  value="@Model.PoolRate" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_FloorsMoney">保底金额:</label>
                <input type="text" id="Control_FloorsMoney" name="FloorsMoney"  value="@Model.FloorsMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_ExcessRate">超额扣率:</label>
                <input type="text" id="Control_ExcessRate" name="ExcessRate"  value="@Model.ExcessRate" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_CreateDate">创建时间:</label>
                <input type="text" id="Control_CreateDate" name="CreateDate"  value="@Model.CreateDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_Operator">操作员:</label>
                <input type="text"  value="@Model.Operator" class="mini-textbox" name="Operator" id="Control_Operator" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_Assessor">审核人:</label>
                <input type="text"  value="@Model.Assessor" class="mini-textbox" name="Assessor" id="Control_Assessor" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_IfExamine">审核:</label>
                <div id="Control_IfExamine" name="IfExamine" value="@Model.IfExamine" class="mini-checkbox"  readOnly="true" text="" trueValue="1" falseValue="0"></div>
            </div>

            <div class="ablock">
                <label for="Control_ExamineDate">审核时间:</label>
                <input type="text" id="Control_ExamineDate" name="ExamineDate"  value="@Model.ExamineDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_OperatorDate">操作时间:</label>
                <input type="text" id="Control_OperatorDate" name="OperatorDate"  value="@Model.OperatorDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="button" class="myButton" id="btnExame" value="审核(Ctrl+E)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

