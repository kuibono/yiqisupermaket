@model YiQiWorkFlow.Domain.Fb.FbPaBaseSet
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>基础参数设置编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "基础参数设置"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/15 19:58:49";var id="@Model.Id";pageName="FbPaBaseSetEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Fb/SaveFbPaBaseSet",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_FbPaBaseSet .content").first().text(r.Message);
                        $("#Tooltip_Critical_FbPaBaseSet").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_FbPaBaseSet .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_FbPaBaseSet").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            基础参数设置编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_FbPaBaseSet">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于基础参数设置编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_FbPaBaseSet">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于基础参数设置编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_FbPaBaseSet">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于基础参数设置编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_FbPaBaseSet">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于基础参数设置编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_GoodsGbLen">大类位数:</label>
                <input type="text" id="Control_GoodsGbLen" name="GoodsGbLen"  value="@Model.GoodsGbLen" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_GoodsGmLen">中类位数:</label>
                <input type="text" id="Control_GoodsGmLen" name="GoodsGmLen"  value="@Model.GoodsGmLen" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_GoodsGsLen">小类位数:</label>
                <input type="text" id="Control_GoodsGsLen" name="GoodsGsLen"  value="@Model.GoodsGsLen" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_GoodsGlLen">细类位数:</label>
                <input type="text" id="Control_GoodsGlLen" name="GoodsGlLen"  value="@Model.GoodsGlLen" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_GoodsClassLevel">1，2，3，4:</label>
                <input type="text" id="Control_GoodsClassLevel" name="GoodsClassLevel"  value="@Model.GoodsClassLevel" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_GoodsLen">商品流水码位数:</label>
                <input type="text" id="Control_GoodsLen" name="GoodsLen"  value="@Model.GoodsLen" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_GoodsPrefix">大中小细:</label>
                <input type="text"  value="@Model.GoodsPrefix" class="mini-textbox" name="GoodsPrefix" id="Control_GoodsPrefix" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_SupLen">供货商流水码位数:</label>
                <input type="text" id="Control_SupLen" name="SupLen"  value="@Model.SupLen" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_SupPrefixType">无，经营方式，指定:</label>
                <input type="text"  value="@Model.SupPrefixType" class="mini-textbox" name="SupPrefixType" id="Control_SupPrefixType" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_SupPrefixContent">前缀内容:</label>
                <input type="text"  value="@Model.SupPrefixContent" class="mini-textbox" name="SupPrefixContent" id="Control_SupPrefixContent" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_PurchasePreciseLen">进价小数位数:</label>
                <input type="text" id="Control_PurchasePreciseLen" name="PurchasePreciseLen"  value="@Model.PurchasePreciseLen" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_SalePreciseLen">销售价小数位数:</label>
                <input type="text" id="Control_SalePreciseLen" name="SalePreciseLen"  value="@Model.SalePreciseLen" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_Operator">操作员:</label>
                <input type="text"  value="@Model.Operator" class="mini-textbox" name="Operator" id="Control_Operator" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_OperatorDate">操作时间:</label>
                <input type="text" id="Control_OperatorDate" name="OperatorDate"  value="@Model.OperatorDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_Id">:</label>
                <input id="Control_Id" value="@Model.Id" name="Id" class="mini-hidden"/>
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

