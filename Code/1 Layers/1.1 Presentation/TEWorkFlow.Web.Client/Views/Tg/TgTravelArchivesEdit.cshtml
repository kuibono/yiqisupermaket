@model YiQiWorkFlow.Domain.Tg.TgTravelArchives
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>旅行社档案编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "旅行社档案"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/13 0:05:08";var id="@Model.Id";pageName="TgTravelArchivesEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "SaveTgTravelArchives",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_TgTravelArchives .content").first().text(r.Message);
                        $("#Tooltip_Critical_TgTravelArchives").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_TgTravelArchives .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_TgTravelArchives").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            旅行社档案编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_TgTravelArchives">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于旅行社档案编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_TgTravelArchives">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于旅行社档案编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_TgTravelArchives">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于旅行社档案编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_TgTravelArchives">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于旅行社档案编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock" style="display:none">
                <label for="Control_TaCode">旅行社编号:</label>
                <input id="Control_TaCode" value="@Model.Id" name="TaCode" class="mini-hidden"/>
            </div>

            <div class="ablock">
                <label for="Control_TaName">旅行社名称:</label>
                <input type="text"  value="@Model.TaName" class="mini-textbox" name="TaName" id="Control_TaName" vtype="maxLength:100" />
            </div>

            <div class="ablock">
                <label for="Control_PyCode">拼音码:</label>
                <input type="text"  value="@Model.PyCode" class="mini-textbox" name="PyCode" id="Control_PyCode" vtype="maxLength:250" />
            </div>

            <div class="ablock">
                <label for="Control_TaTypeCode">旅行社类型:</label>
                <input type="text"  value="@Model.TaTypeCode" class="mini-textbox" name="TaTypeCode" id="Control_TaTypeCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_PushRate">提成比率:</label>
                <input type="text" id="Control_PushRate" name="PushRate"  value="@Model.PushRate" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="6"/>
            </div>

            <div class="ablock">
                <label for="Control_PushMoney">人次提成金额:</label>
                <input type="text" id="Control_PushMoney" name="PushMoney"  value="@Model.PushMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="6"/>
            </div>

            <div class="ablock">
                <label for="Control_PushMoneyAccount">人次挂帐金额:</label>
                <input type="text" id="Control_PushMoneyAccount" name="PushMoneyAccount"  value="@Model.PushMoneyAccount" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="6"/>
            </div>

            <div class="ablock">
                <label for="Control_TaReturnsRate">旅行社返利比率:</label>
                <input type="text" id="Control_TaReturnsRate" name="TaReturnsRate"  value="@Model.TaReturnsRate" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="6"/>
            </div>

            <div class="ablock">
                <label for="Control_Functionary">负责人:</label>
                <input type="text"  value="@Model.Functionary" class="mini-textbox" name="Functionary" id="Control_Functionary" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_FunctionaryPhone">负责人电话:</label>
                <input type="text"  value="@Model.FunctionaryPhone" class="mini-textbox" name="FunctionaryPhone" id="Control_FunctionaryPhone" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_Linkman">联系人:</label>
                <input type="text"  value="@Model.Linkman" class="mini-textbox" name="Linkman" id="Control_Linkman" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_ContactPhone">联系人电话:</label>
                <input type="text"  value="@Model.ContactPhone" class="mini-textbox" name="ContactPhone" id="Control_ContactPhone" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_ContactAddress">联系地址:</label>
                <input type="text"  value="@Model.ContactAddress" class="mini-textbox" name="ContactAddress" id="Control_ContactAddress" vtype="maxLength:100" />
            </div>

            <div class="ablock">
                <label for="Control_OfficePhone">办公电话:</label>
                <input type="text"  value="@Model.OfficePhone" class="mini-textbox" name="OfficePhone" id="Control_OfficePhone" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_FaxPhone">传真:</label>
                <input type="text"  value="@Model.FaxPhone" class="mini-textbox" name="FaxPhone" id="Control_FaxPhone" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_eMail">邮箱:</label>
                <input type="text"  value="@Model.eMail" class="mini-textbox" name="eMail" id="Control_eMail" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_Postalcode">邮政编码:</label>
                <input type="text"  value="@Model.Postalcode" class="mini-textbox" name="Postalcode" id="Control_Postalcode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_OpenAccount">开户行:</label>
                <input type="text"  value="@Model.OpenAccount" class="mini-textbox" name="OpenAccount" id="Control_OpenAccount" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_BankAccount">帐号:</label>
                <input type="text"  value="@Model.BankAccount" class="mini-textbox" name="BankAccount" id="Control_BankAccount" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_TaxNumber">税号:</label>
                <input type="text"  value="@Model.TaxNumber" class="mini-textbox" name="TaxNumber" id="Control_TaxNumber" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_PrintType">打印方式:</label>
                <input type="text"  value="@Model.PrintType" class="mini-textbox" name="PrintType" id="Control_PrintType" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_IfPrint">是否打印:</label>
                <div id="Control_IfPrint" name="IfPrint" value="1" class="mini-checkbox" checked="true" readOnly="false" text="打印"></div>
            </div>

            <div class="ablock">
                <label for="Control_CarsType">车停结算:</label>
                <input type="text"  value="@Model.CarsType" class="mini-textbox" name="CarsType" id="Control_CarsType" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_Comment">备注:</label>
                <input type="text"  value="@Model.Comment" class="mini-textbox" name="Comment" id="Control_Comment" vtype="maxLength:50" />
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

