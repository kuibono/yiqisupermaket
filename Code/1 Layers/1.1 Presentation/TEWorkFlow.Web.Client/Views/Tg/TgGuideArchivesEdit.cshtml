@model YiQiWorkFlow.Domain.Tg.TgGuideArchives
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>导游人员信息编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "导游人员信息"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/13 0:05:08";var id="@Model.Id";pageName="TgGuideArchivesEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "SaveTgGuideArchives",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_TgGuideArchives .content").first().text(r.Message);
                        $("#Tooltip_Critical_TgGuideArchives").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_TgGuideArchives .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_TgGuideArchives").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            导游人员信息编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_TgGuideArchives">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于导游人员信息编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_TgGuideArchives">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于导游人员信息编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_TgGuideArchives">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于导游人员信息编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_TgGuideArchives">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于导游人员信息编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_TgFlowNumber">流水单号:</label>
                <input type="text" id="Control_TgFlowNumber" name="TgFlowNumber"  value="@Model.TgFlowNumber" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="0"/>
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_GuideCode">人员编号:</label>
                <input id="Control_GuideCode" value="@Model.Id" name="GuideCode" class="mini-hidden"/>
            </div>

            <div class="ablock">
                <label for="Control_GuideName">人员姓名:</label>
                <input type="text"  value="@Model.GuideName" class="mini-textbox" name="GuideName" id="Control_GuideName" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_PyCode">拼音码:</label>
                <input type="text"  value="@Model.PyCode" class="mini-textbox" name="PyCode" id="Control_PyCode" vtype="maxLength:250" />
            </div>

            <div class="ablock">
                <label for="Control_GuideType">人员职业:</label>
                <input type="text"  value="@Model.GuideType" class="mini-textbox" name="GuideType" id="Control_GuideType" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GuideId">人员证件:</label>
                <input type="text"  value="@Model.GuideId" class="mini-textbox" name="GuideId" id="Control_GuideId" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_ContactPhone">联系人电话:</label>
                <input type="text"  value="@Model.ContactPhone" class="mini-textbox" name="ContactPhone" id="Control_ContactPhone" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_TaCode">所属旅行社:</label>
                <input type="text"  value="@Model.TaCode" class="mini-textbox" name="TaCode" id="Control_TaCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_TaName">旅行社名称:</label>
                <input type="text"  value="@Model.TaName" class="mini-textbox" name="TaName" id="Control_TaName" vtype="maxLength:100" />
            </div>

            <div class="ablock">
                <label for="Control_TacCode">策略编码:</label>
                <input type="text"  value="@Model.TacCode" class="mini-textbox" name="TacCode" id="Control_TacCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_PushRate">提成比率:</label>
                <input type="text" id="Control_PushRate" name="PushRate"  value="@Model.PushRate" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="6"/>
            </div>

            <div class="ablock">
                <label for="Control_PushMoney">人次提成金额:</label>
                <input type="text" id="Control_PushMoney" name="PushMoney"  value="@Model.PushMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="6"/>
            </div>

            <div class="ablock">
                <label for="Control_LicensePlate">车牌号码:</label>
                <input type="text"  value="@Model.LicensePlate" class="mini-textbox" name="LicensePlate" id="Control_LicensePlate" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_Comment">备注:</label>
                <input type="text"  value="@Model.Comment" class="mini-textbox" name="Comment" id="Control_Comment" vtype="maxLength:50" />
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

