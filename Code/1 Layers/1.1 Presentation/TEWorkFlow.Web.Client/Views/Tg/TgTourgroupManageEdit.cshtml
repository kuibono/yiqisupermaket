@model YiQiWorkFlow.Domain.Tg.TgTourgroupManage
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>团队购物管理编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "团队购物管理"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/13 20:26:59";var id="@Model.Id";pageName="TgTourgroupManageEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Tg/SaveTgTourgroupManage",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_TgTourgroupManage .content").first().text(r.Message);
                        $("#Tooltip_Critical_TgTourgroupManage").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_TgTourgroupManage .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_TgTourgroupManage").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            团队购物管理编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_TgTourgroupManage">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于团队购物管理编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_TgTourgroupManage">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于团队购物管理编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_TgTourgroupManage">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于团队购物管理编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_TgTourgroupManage">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于团队购物管理编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_TgFlowNumber">流水单号:</label>
                <input type="text" id="Control_TgFlowNumber" name="TgFlowNumber"  value="@Model.TgFlowNumber" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="0"/>
            </div>

            <div class="ablock">
                <label for="Control_GroupNumber">团队编号:</label>
                <input type="text"  value="@Model.GroupNumber" class="mini-textbox" name="GroupNumber" id="Control_GroupNumber" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GroupDate">开团日期:</label>
                <input type="text" id="Control_GroupDate" name="GroupDate"  value="@Model.GroupDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_GroupTypeCode">团队类型:</label>
                <input type="text"  value="@Model.GroupTypeCode" class="mini-textbox" name="GroupTypeCode" id="Control_GroupTypeCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_TaCode">所属旅行社:</label>
                <input type="text"  value="@Model.TaCode" class="mini-textbox" name="TaCode" id="Control_TaCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_TaName">旅行社名称:</label>
                <input type="text"  value="@Model.TaName" class="mini-textbox" name="TaName" id="Control_TaName" vtype="maxLength:100" />
            </div>

            <div class="ablock">
                <label for="Control_TaTypeCode">旅行社类型:</label>
                <input type="text"  value="@Model.TaTypeCode" class="mini-textbox" name="TaTypeCode" id="Control_TaTypeCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GuideCode">导游编号:</label>
                <input type="text"  value="@Model.GuideCode" class="mini-textbox" name="GuideCode" id="Control_GuideCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GuideName">导游名称:</label>
                <input type="text"  value="@Model.GuideName" class="mini-textbox" name="GuideName" id="Control_GuideName" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GuideTacCode">导游策略编码:</label>
                <input type="text"  value="@Model.GuideTacCode" class="mini-textbox" name="GuideTacCode" id="Control_GuideTacCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GuidePushRate">导游提成比率:</label>
                <input type="text" id="Control_GuidePushRate" name="GuidePushRate"  value="@Model.GuidePushRate" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="6"/>
            </div>

            <div class="ablock">
                <label for="Control_GuidePushMoney">导游人次提成:</label>
                <input type="text" id="Control_GuidePushMoney" name="GuidePushMoney"  value="@Model.GuidePushMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="6"/>
            </div>

            <div class="ablock">
                <label for="Control_PersonTimes">团队人数:</label>
                <input type="text" id="Control_PersonTimes" name="PersonTimes"  value="@Model.PersonTimes" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="0"/>
            </div>

            <div class="ablock">
                <label for="Control_DiscountRate">购物折扣:</label>
                <input type="text" id="Control_DiscountRate" name="DiscountRate"  value="@Model.DiscountRate" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_ExpostorTacCode">讲解员策略编码:</label>
                <input type="text"  value="@Model.ExpostorTacCode" class="mini-textbox" name="ExpostorTacCode" id="Control_ExpostorTacCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_Expostor">讲解员:</label>
                <input type="text"  value="@Model.Expostor" class="mini-textbox" name="Expostor" id="Control_Expostor" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_ExpostorPushRate">讲解员提成:</label>
                <input type="text" id="Control_ExpostorPushRate" name="ExpostorPushRate"  value="@Model.ExpostorPushRate" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_ExpostorPushMoney">讲解员人次提成:</label>
                <input type="text" id="Control_ExpostorPushMoney" name="ExpostorPushMoney"  value="@Model.ExpostorPushMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_Driver">司机:</label>
                <input type="text"  value="@Model.Driver" class="mini-textbox" name="Driver" id="Control_Driver" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_LicensePlate">车牌号码:</label>
                <input type="text"  value="@Model.LicensePlate" class="mini-textbox" name="LicensePlate" id="Control_LicensePlate" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_CarsGroup">车队:</label>
                <input type="text"  value="@Model.CarsGroup" class="mini-textbox" name="CarsGroup" id="Control_CarsGroup" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_CarsTypeCode">汽车型号:</label>
                <input type="text"  value="@Model.CarsTypeCode" class="mini-textbox" name="CarsTypeCode" id="Control_CarsTypeCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_StopCharge">停车费:</label>
                <input type="text" id="Control_StopCharge" name="StopCharge"  value="@Model.StopCharge" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_CustomerFrom">顾客来源:</label>
                <input type="text"  value="@Model.CustomerFrom" class="mini-textbox" name="CustomerFrom" id="Control_CustomerFrom" vtype="maxLength:100" />
            </div>

            <div class="ablock">
                <label for="Control_Comment">备注:</label>
                <input type="text"  value="@Model.Comment" class="mini-textbox" name="Comment" id="Control_Comment" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_CreateDate">创建时间:</label>
                <input type="text" id="Control_CreateDate" name="CreateDate"  value="@Model.CreateDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_Operator">操作员:</label>
                <input type="text"  value="@Model.Operator" class="mini-textbox" name="Operator" id="Control_Operator" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_Assessor">审核人:</label>
                <input type="text"  value="@Model.Assessor" class="mini-textbox" name="Assessor" id="Control_Assessor" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_IfExamine">是否审核:</label>
                <div id="Control_IfExamine" name="IfExamine" value="1" class="mini-checkbox" checked="true" readOnly="false" text="审核"></div>
            </div>

            <div class="ablock">
                <label for="Control_ExamineDate">审核时间:</label>
                <input type="text" id="Control_ExamineDate" name="ExamineDate"  value="@Model.ExamineDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_OperatorDate">操作时间:</label>
                <input type="text" id="Control_OperatorDate" name="OperatorDate"  value="@Model.OperatorDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_Id">:</label>
                <input id="Control_Id" value="@Model.Id" name="Id" class="mini-hidden"/>
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

