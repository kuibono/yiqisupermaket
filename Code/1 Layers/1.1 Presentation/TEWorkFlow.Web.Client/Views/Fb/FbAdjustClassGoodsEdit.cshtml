@model YiQiWorkFlow.Domain.Fb.FbAdjustClassGoods
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>调商品所属类别商品明细编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "调商品所属类别商品明细"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/13 20:26:45";var id="@Model.Id";pageName="FbAdjustClassGoodsEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Fb/SaveFbAdjustClassGoods",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_FbAdjustClassGoods .content").first().text(r.Message);
                        $("#Tooltip_Critical_FbAdjustClassGoods").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_FbAdjustClassGoods .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_FbAdjustClassGoods").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            调商品所属类别商品明细编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_FbAdjustClassGoods">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于调商品所属类别商品明细编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_FbAdjustClassGoods">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于调商品所属类别商品明细编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_FbAdjustClassGoods">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于调商品所属类别商品明细编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_FbAdjustClassGoods">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于调商品所属类别商品明细编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_AdjustNumber">调整单号:</label>
                <input type="text"  value="@Model.AdjustNumber" class="mini-textbox" name="AdjustNumber" id="Control_AdjustNumber" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsCode">商品编码:</label>
                <input type="text"  value="@Model.GoodsCode" class="mini-textbox" name="GoodsCode" id="Control_GoodsCode" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_GbCodeOld">原大类:</label>
                <input type="text"  value="@Model.GbCodeOld" class="mini-textbox" name="GbCodeOld" id="Control_GbCodeOld" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GmCodeOld">原中类:</label>
                <input type="text"  value="@Model.GmCodeOld" class="mini-textbox" name="GmCodeOld" id="Control_GmCodeOld" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GsCodeOld">原小类:</label>
                <input type="text"  value="@Model.GsCodeOld" class="mini-textbox" name="GsCodeOld" id="Control_GsCodeOld" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GlCodeOld">原细类:</label>
                <input type="text"  value="@Model.GlCodeOld" class="mini-textbox" name="GlCodeOld" id="Control_GlCodeOld" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GbCode">大类:</label>
                <input type="text"  value="@Model.GbCode" class="mini-textbox" name="GbCode" id="Control_GbCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GmCode">中类:</label>
                <input type="text"  value="@Model.GmCode" class="mini-textbox" name="GmCode" id="Control_GmCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GsCode">小类:</label>
                <input type="text"  value="@Model.GsCode" class="mini-textbox" name="GsCode" id="Control_GsCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GlCode">细类:</label>
                <input type="text"  value="@Model.GlCode" class="mini-textbox" name="GlCode" id="Control_GlCode" vtype="maxLength:20" />
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_Id">:</label>
                <input id="Control_Id" value="@Model.Id" name="Id" class="mini-hidden"/>
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

