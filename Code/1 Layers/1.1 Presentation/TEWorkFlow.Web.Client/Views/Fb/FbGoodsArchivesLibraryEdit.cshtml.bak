@model YiQiWorkFlow.Domain.Fb.FbGoodsArchivesLibrary
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>商品条码库编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "商品条码库"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/13 20:26:48";var id="@Model.Id";pageName="FbGoodsArchivesLibraryEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Fb/SaveFbGoodsArchivesLibrary",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_FbGoodsArchivesLibrary .content").first().text(r.Message);
                        $("#Tooltip_Critical_FbGoodsArchivesLibrary").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_FbGoodsArchivesLibrary .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_FbGoodsArchivesLibrary").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            商品条码库编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_FbGoodsArchivesLibrary">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于商品条码库编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_FbGoodsArchivesLibrary">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于商品条码库编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_FbGoodsArchivesLibrary">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于商品条码库编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_FbGoodsArchivesLibrary">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于商品条码库编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock" style="display:none">
                <label for="Control_GoodsBarCode">商品销售码:</label>
                <input id="Control_GoodsBarCode" value="@Model.Id" name="GoodsBarCode" class="mini-hidden"/>
            </div>

            <div class="ablock">
                <label for="Control_GoodsName">商品名称:</label>
                <input type="text"  value="@Model.GoodsName" class="mini-textbox" name="GoodsName" id="Control_GoodsName" vtype="maxLength:250" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsSubName">商品简称:</label>
                <input type="text"  value="@Model.GoodsSubName" class="mini-textbox" name="GoodsSubName" id="Control_GoodsSubName" vtype="maxLength:250" />
            </div>

            <div class="ablock">
                <label for="Control_ProducingArea">产地:</label>
                <input type="text"  value="@Model.ProducingArea" class="mini-textbox" name="ProducingArea" id="Control_ProducingArea" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_Specification">规格:</label>
                <input type="text"  value="@Model.Specification" class="mini-textbox" name="Specification" id="Control_Specification" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_PackUnitCode">包装单位:</label>
                <input type="text"  value="@Model.PackUnitCode" class="mini-textbox" name="PackUnitCode" id="Control_PackUnitCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_OwnerType">从属类型:</label>
                <input type="text"  value="@Model.OwnerType" class="mini-textbox" name="OwnerType" id="Control_OwnerType" vtype="maxLength:2" />
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

