@model YiQiWorkFlow.Domain.Ba.BaSupplierDetail
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>供货商结算明细编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "供货商结算明细"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/13 0:05:03";var id="@Model.Id";pageName="BaSupplierDetailEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "SaveBaSupplierDetail",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_BaSupplierDetail .content").first().text(r.Message);
                        $("#Tooltip_Critical_BaSupplierDetail").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_BaSupplierDetail .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_BaSupplierDetail").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            供货商结算明细编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_BaSupplierDetail">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于供货商结算明细编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_BaSupplierDetail">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于供货商结算明细编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_BaSupplierDetail">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于供货商结算明细编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_BaSupplierDetail">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于供货商结算明细编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_BaNumber">结算单号:</label>
                <input type="text"  value="@Model.BaNumber" class="mini-textbox" name="BaNumber" id="Control_BaNumber" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_OperatNumber">业务单号:</label>
                <input type="text"  value="@Model.OperatNumber" class="mini-textbox" name="OperatNumber" id="Control_OperatNumber" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_OperatType">业务类型:</label>
                <input type="text"  value="@Model.OperatType" class="mini-textbox" name="OperatType" id="Control_OperatType" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_PayableMoney">应付金额:</label>
                <input type="text" id="Control_PayableMoney" name="PayableMoney"  value="@Model.PayableMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_PaidMoney">已付金额:</label>
                <input type="text" id="Control_PaidMoney" name="PaidMoney"  value="@Model.PaidMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_UnpaidMoney">未付金额:</label>
                <input type="text" id="Control_UnpaidMoney" name="UnpaidMoney"  value="@Model.UnpaidMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_PayMoney">付款金额:</label>
                <input type="text" id="Control_PayMoney" name="PayMoney"  value="@Model.PayMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_PayMargin">付款差额:</label>
                <input type="text" id="Control_PayMargin" name="PayMargin"  value="@Model.PayMargin" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_MarginDeal">结转，优惠:</label>
                <input type="text"  value="@Model.MarginDeal" class="mini-textbox" name="MarginDeal" id="Control_MarginDeal" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_TableName">业务表名:</label>
                <input type="text"  value="@Model.TableName" class="mini-textbox" name="TableName" id="Control_TableName" vtype="maxLength:40" />
            </div>

            <div class="ablock">
                <label for="Control_ColumnName">业务主键:</label>
                <input type="text"  value="@Model.ColumnName" class="mini-textbox" name="ColumnName" id="Control_ColumnName" vtype="maxLength:20" />
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_Id">:</label>
                <input id="Control_Id" value="@Model.Id" name="Id" class="mini-hidden"/>
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

