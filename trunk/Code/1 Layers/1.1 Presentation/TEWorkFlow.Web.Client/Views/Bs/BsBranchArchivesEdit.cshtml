@model YiQiWorkFlow.Domain.Bs.BsBranchArchives
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>分店档案管理编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "分店档案管理"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/13 20:26:42";var id="@Model.Id";pageName="BsBranchArchivesEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Bs/SaveBsBranchArchives",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_BsBranchArchives .content").first().text(r.Message);
                        $("#Tooltip_Critical_BsBranchArchives").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_BsBranchArchives .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_BsBranchArchives").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            分店档案管理编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_BsBranchArchives">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于分店档案管理编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_BsBranchArchives">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于分店档案管理编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_BsBranchArchives">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于分店档案管理编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_BsBranchArchives">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于分店档案管理编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock" style="display:none">
                <label for="Control_bCode">分店编码:</label>
                <input id="Control_bCode" value="@Model.Id" name="bCode" class="mini-hidden"/>
            </div>

            <div class="ablock">
                <label for="Control_bName">分店名称:</label>
                <input type="text"  value="@Model.bName" class="mini-textbox" name="bName" id="Control_bName" vtype="maxLength:40" />
            </div>

            <div class="ablock">
                <label for="Control_PyCode">拼音码:</label>
                <input type="text"  value="@Model.PyCode" class="mini-textbox" name="PyCode" id="Control_PyCode" vtype="maxLength:250" />
            </div>

            <div class="ablock">
                <label for="Control_bType">分店类型:</label>
                <input type="text"  value="@Model.bType" class="mini-textbox" name="bType" id="Control_bType" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_bState">分店状态:</label>
                <input type="text"  value="@Model.bState" class="mini-textbox" name="bState" id="Control_bState" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_AreaCode">所属区域:</label>
                <input type="text"  value="@Model.AreaCode" class="mini-textbox" name="AreaCode" id="Control_AreaCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_ClassCode">所属分类:</label>
                <input type="text"  value="@Model.ClassCode" class="mini-textbox" name="ClassCode" id="Control_ClassCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_Functionary">负责人:</label>
                <input type="text"  value="@Model.Functionary" class="mini-textbox" name="Functionary" id="Control_Functionary" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_FunctionaryPhone">负责人电话:</label>
                <input type="text"  value="@Model.FunctionaryPhone" class="mini-textbox" name="FunctionaryPhone" id="Control_FunctionaryPhone" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_ContactAddress">联系地址:</label>
                <input type="text"  value="@Model.ContactAddress" class="mini-textbox" name="ContactAddress" id="Control_ContactAddress" vtype="maxLength:100" />
            </div>

            <div class="ablock">
                <label for="Control_OfficePhone">办公电话:</label>
                <input type="text"  value="@Model.OfficePhone" class="mini-textbox" name="OfficePhone" id="Control_OfficePhone" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_FaxPhone">传真:</label>
                <input type="text"  value="@Model.FaxPhone" class="mini-textbox" name="FaxPhone" id="Control_FaxPhone" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_eMail">邮箱:</label>
                <input type="text"  value="@Model.eMail" class="mini-textbox" name="eMail" id="Control_eMail" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_Postalcode">邮政编码:</label>
                <input type="text"  value="@Model.Postalcode" class="mini-textbox" name="Postalcode" id="Control_Postalcode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_OpenAccount">开户行:</label>
                <input type="text"  value="@Model.OpenAccount" class="mini-textbox" name="OpenAccount" id="Control_OpenAccount" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_BankAccount">帐号:</label>
                <input type="text"  value="@Model.BankAccount" class="mini-textbox" name="BankAccount" id="Control_BankAccount" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_TaxNumber">税号:</label>
                <input type="text"  value="@Model.TaxNumber" class="mini-textbox" name="TaxNumber" id="Control_TaxNumber" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_OpCode">1经销，2代销，3联营:</label>
                <input type="text"  value="@Model.OpCode" class="mini-textbox" name="OpCode" id="Control_OpCode" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_StockVoucher">普通发票，增殖税发票:</label>
                <input type="text"  value="@Model.StockVoucher" class="mini-textbox" name="StockVoucher" id="Control_StockVoucher" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_InputTax">进项税率:</label>
                <input type="text" id="Control_InputTax" name="InputTax"  value="@Model.InputTax" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_BalanceMode">预付货款，货到付款，实销实付，扣点结算:</label>
                <input type="text"  value="@Model.BalanceMode" class="mini-textbox" name="BalanceMode" id="Control_BalanceMode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_PayMode">现金，支票，汇票:</label>
                <input type="text"  value="@Model.PayMode" class="mini-textbox" name="PayMode" id="Control_PayMode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_BalancePeriod">月结算，季计算，年结算:</label>
                <input type="text"  value="@Model.BalancePeriod" class="mini-textbox" name="BalancePeriod" id="Control_BalancePeriod" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_BalanceDay">31:</label>
                <input type="text" id="Control_BalanceDay" name="BalanceDay"  value="@Model.BalanceDay" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_DeliveryDays">送货天数:</label>
                <input type="text" id="Control_DeliveryDays" name="DeliveryDays"  value="@Model.DeliveryDays" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_SupplyPriceType">配价方式:</label>
                <input type="text"  value="@Model.SupplyPriceType" class="mini-textbox" name="SupplyPriceType" id="Control_SupplyPriceType" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GrossRate">加价率:</label>
                <input type="text" id="Control_GrossRate" name="GrossRate"  value="@Model.GrossRate" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_WhCode">配送仓库:</label>
                <input type="text"  value="@Model.WhCode" class="mini-textbox" name="WhCode" id="Control_WhCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_CreateDate">创建时间:</label>
                <input type="text" id="Control_CreateDate" name="CreateDate"  value="@Model.CreateDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_Operator">操作员:</label>
                <input type="text"  value="@Model.Operator" class="mini-textbox" name="Operator" id="Control_Operator" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_Assessor">审核人:</label>
                <input type="text"  value="@Model.Assessor" class="mini-textbox" name="Assessor" id="Control_Assessor" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_IfExamine">0否，1是:</label>
                <div id="Control_IfExamine" name="IfExamine" value="1" class="mini-checkbox" checked="true" readOnly="false" text="0否，1是"></div>
            </div>

            <div class="ablock">
                <label for="Control_ExamineDate">审核时间:</label>
                <input type="text" id="Control_ExamineDate" name="ExamineDate"  value="@Model.ExamineDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_OperatorDate">操作时间:</label>
                <input type="text" id="Control_OperatorDate" name="OperatorDate"  value="@Model.OperatorDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

