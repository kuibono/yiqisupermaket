@model YiQiWorkFlow.Domain.Op.OpAdjustPurchaseDetail
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>进货成本调整批次明细编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "进货成本调整批次明细"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/15 19:58:52";var id="@Model.Id";pageName="OpAdjustPurchaseDetailEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Op/SaveOpAdjustPurchaseDetail",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_OpAdjustPurchaseDetail .content").first().text(r.Message);
                        $("#Tooltip_Critical_OpAdjustPurchaseDetail").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_OpAdjustPurchaseDetail .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_OpAdjustPurchaseDetail").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            进货成本调整批次明细编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_OpAdjustPurchaseDetail">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于进货成本调整批次明细编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_OpAdjustPurchaseDetail">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于进货成本调整批次明细编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_OpAdjustPurchaseDetail">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于进货成本调整批次明细编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_OpAdjustPurchaseDetail">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于进货成本调整批次明细编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_AdjustNumber">调整单号:</label>
                <input type="text"  value="@Model.AdjustNumber" class="mini-textbox" name="AdjustNumber" id="Control_AdjustNumber" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_AdjustDate">调整日期:</label>
                <input type="text" id="Control_AdjustDate" name="AdjustDate"  value="@Model.AdjustDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_WhCode">仓库编码:</label>
                <input type="text"  value="@Model.WhCode" class="mini-textbox" name="WhCode" id="Control_WhCode" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_BatchNumber">批次编号:</label>
                <input type="text" id="Control_BatchNumber" name="BatchNumber"  value="@Model.BatchNumber" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_PiNumber">入库单号:</label>
                <input type="text"  value="@Model.PiNumber" class="mini-textbox" name="PiNumber" id="Control_PiNumber" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_PutinDate">入库日期:</label>
                <input type="text" id="Control_PutinDate" name="PutinDate"  value="@Model.PutinDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="true" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsCode">商品编码:</label>
                <input type="text"  value="@Model.GoodsCode" class="mini-textbox" name="GoodsCode" id="Control_GoodsCode" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsBarCode">商品销售码:</label>
                <input type="text"  value="@Model.GoodsBarCode" class="mini-textbox" name="GoodsBarCode" id="Control_GoodsBarCode" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_SupCode">供货商编码:</label>
                <input type="text"  value="@Model.SupCode" class="mini-textbox" name="SupCode" id="Control_SupCode" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_InputTax">进项税率:</label>
                <input type="text" id="Control_InputTax" name="InputTax"  value="@Model.InputTax" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_PurchasePrice">含税进价:</label>
                <input type="text" id="Control_PurchasePrice" name="PurchasePrice"  value="@Model.PurchasePrice" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="6"/>
            </div>

            <div class="ablock">
                <label for="Control_NontaxPurchasePrice">不含税进价:</label>
                <input type="text" id="Control_NontaxPurchasePrice" name="NontaxPurchasePrice"  value="@Model.NontaxPurchasePrice" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="6"/>
            </div>

            <div class="ablock">
                <label for="Control_SalePrice">销售价:</label>
                <input type="text" id="Control_SalePrice" name="SalePrice"  value="@Model.SalePrice" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_NontaxSalePrice">不含税售价:</label>
                <input type="text" id="Control_NontaxSalePrice" name="NontaxSalePrice"  value="@Model.NontaxSalePrice" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_PurchaseQty">进货数量:</label>
                <input type="text" id="Control_PurchaseQty" name="PurchaseQty"  value="@Model.PurchaseQty" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_StockoutQty">冲减数量:</label>
                <input type="text" id="Control_StockoutQty" name="StockoutQty"  value="@Model.StockoutQty" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_StockQty">剩余数量:</label>
                <input type="text" id="Control_StockQty" name="StockQty"  value="@Model.StockQty" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_AdjustQty">调整数量:</label>
                <input type="text" id="Control_AdjustQty" name="AdjustQty"  value="@Model.AdjustQty" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_AdjustPrice">调整进价:</label>
                <input type="text" id="Control_AdjustPrice" name="AdjustPrice"  value="@Model.AdjustPrice" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_AdjustDiff">调整差价:</label>
                <input type="text" id="Control_AdjustDiff" name="AdjustDiff"  value="@Model.AdjustDiff" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_AdjustMoney">调整金额:</label>
                <input type="text" id="Control_AdjustMoney" name="AdjustMoney"  value="@Model.AdjustMoney" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_IfAblebalance">是否允许结算:</label>
                <div id="Control_IfAblebalance" name="IfAblebalance" value="1" class="mini-checkbox" checked="true" readOnly="false" text="允许结算"></div>
            </div>

            <div class="ablock">
                <label for="Control_AdjustBaMoney">调整结算金额:</label>
                <input type="text" id="Control_AdjustBaMoney" name="AdjustBaMoney"  value="@Model.AdjustBaMoney" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_IfBalance">是否结算:</label>
                <div id="Control_IfBalance" name="IfBalance" value="1" class="mini-checkbox" checked="true" readOnly="false" text="结算"></div>
            </div>

            <div class="ablock">
                <label for="Control_BaNumber">结算单号:</label>
                <input type="text"  value="@Model.BaNumber" class="mini-textbox" name="BaNumber" id="Control_BaNumber" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_BaMoney">结算金额:</label>
                <input type="text" id="Control_BaMoney" name="BaMoney"  value="@Model.BaMoney" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="4"/>
            </div>

            <div class="ablock">
                <label for="Control_ProduceDate">生产日期:</label>
                <input type="text" id="Control_ProduceDate" name="ProduceDate"  value="@Model.ProduceDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_Id">:</label>
                <input id="Control_Id" value="@Model.Id" name="Id" class="mini-hidden"/>
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

