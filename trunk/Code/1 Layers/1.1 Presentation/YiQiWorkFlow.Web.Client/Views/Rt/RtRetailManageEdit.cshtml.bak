@model YiQiWorkFlow.Domain.Rt.RtRetailManage
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>销售主表编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "销售主表"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/13 20:26:56";var id="@Model.Id";pageName="RtRetailManageEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Rt/SaveRtRetailManage",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_RtRetailManage .content").first().text(r.Message);
                        $("#Tooltip_Critical_RtRetailManage").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_RtRetailManage .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_RtRetailManage").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            销售主表编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_RtRetailManage">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于销售主表编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_RtRetailManage">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于销售主表编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_RtRetailManage">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于销售主表编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_RtRetailManage">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于销售主表编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_SaFlowNumber">流水号:</label>
                <input type="text" id="Control_SaFlowNumber" name="SaFlowNumber"  value="@Model.SaFlowNumber" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="0"/>
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_RtNumber">销售单号:</label>
                <input id="Control_RtNumber" value="@Model.Id" name="RtNumber" class="mini-hidden"/>
            </div>

            <div class="ablock">
                <label for="Control_RtDate">销售时间:</label>
                <input type="text" id="Control_RtDate" name="RtDate"  value="@Model.RtDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_RtType">销售方式:</label>
                <input type="text"  value="@Model.RtType" class="mini-textbox" name="RtType" id="Control_RtType" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_WhCode">仓库编码:</label>
                <input type="text"  value="@Model.WhCode" class="mini-textbox" name="WhCode" id="Control_WhCode" vtype="maxLength:20" />
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_bCode">分店编码:</label>
                <input id="Control_bCode" value="@Model.Id" name="bCode" class="mini-hidden"/>
            </div>

            <div class="ablock">
                <label for="Control_Posid">Pos机号:</label>
                <input type="text"  value="@Model.Posid" class="mini-textbox" name="Posid" id="Control_Posid" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_SaleQty">数量:</label>
                <input type="text" id="Control_SaleQty" name="SaleQty"  value="@Model.SaleQty" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_SaleMoney">销售金额:</label>
                <input type="text" id="Control_SaleMoney" name="SaleMoney"  value="@Model.SaleMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_DiscountMoney">折扣金额:</label>
                <input type="text" id="Control_DiscountMoney" name="DiscountMoney"  value="@Model.DiscountMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_CardNumber">会员卡号:</label>
                <input type="text"  value="@Model.CardNumber" class="mini-textbox" name="CardNumber" id="Control_CardNumber" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_Cashier">收银员:</label>
                <input type="text"  value="@Model.Cashier" class="mini-textbox" name="Cashier" id="Control_Cashier" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_CashierHigher">授权人员:</label>
                <input type="text"  value="@Model.CashierHigher" class="mini-textbox" name="CashierHigher" id="Control_CashierHigher" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_Salesman">营业员:</label>
                <input type="text"  value="@Model.Salesman" class="mini-textbox" name="Salesman" id="Control_Salesman" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_UnderCounterCode">柜台:</label>
                <input type="text"  value="@Model.UnderCounterCode" class="mini-textbox" name="UnderCounterCode" id="Control_UnderCounterCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_ShiftDate">班次时间:</label>
                <input type="text" id="Control_ShiftDate" name="ShiftDate"  value="@Model.ShiftDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_ReturnNumber">返货单号:</label>
                <input type="text"  value="@Model.ReturnNumber" class="mini-textbox" name="ReturnNumber" id="Control_ReturnNumber" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_Comment">备注:</label>
                <input type="text"  value="@Model.Comment" class="mini-textbox" name="Comment" id="Control_Comment" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_SysGuid">系统标识:</label>
                <input type="text"  value="@Model.SysGuid" class="mini-textbox" name="SysGuid" id="Control_SysGuid" vtype="maxLength:60" />
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

