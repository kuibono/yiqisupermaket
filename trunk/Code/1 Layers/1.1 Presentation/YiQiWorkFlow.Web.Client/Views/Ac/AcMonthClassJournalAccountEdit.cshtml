    <script type="text/javascript"> var pageTitle = "类别进销存月帐"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/15 19:58:30";var id="@Model.Id";pageName="AcMonthClassJournalAccountEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Ac/SaveAcMonthClassJournalAccount",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_AcMonthClassJournalAccount .content").first().text(r.Message);
                        $("#Tooltip_Critical_AcMonthClassJournalAccount").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_AcMonthClassJournalAccount .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_AcMonthClassJournalAccount").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
