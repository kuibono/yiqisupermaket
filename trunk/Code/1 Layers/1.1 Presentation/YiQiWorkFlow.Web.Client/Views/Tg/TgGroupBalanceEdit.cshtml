@model YiQiWorkFlow.Domain.Tg.TgGroupBalance
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>团队结算管理编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "团队结算管理"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/13 20:26:58";var id="@Model.Id";pageName="TgGroupBalanceEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Tg/SaveTgGroupBalance",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_TgGroupBalance .content").first().text(r.Message);
                        $("#Tooltip_Critical_TgGroupBalance").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_TgGroupBalance .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_TgGroupBalance").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            团队结算管理编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_TgGroupBalance">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于团队结算管理编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_TgGroupBalance">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于团队结算管理编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_TgGroupBalance">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于团队结算管理编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_TgGroupBalance">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于团队结算管理编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock" style="display:none">
                <label for="Control_BaNumber">结算单号:</label>
                <input id="Control_BaNumber" value="@Model.Id" name="BaNumber" class="mini-hidden"/>
            </div>

            <div class="ablock">
                <label for="Control_BaDate">结算日期:</label>
                <input type="text" id="Control_BaDate" name="BaDate"  value="@Model.BaDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_PaywayCode">付款方式:</label>
                <input type="text"  value="@Model.PaywayCode" class="mini-textbox" name="PaywayCode" id="Control_PaywayCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_EnCode">企业编码:</label>
                <input type="text"  value="@Model.EnCode" class="mini-textbox" name="EnCode" id="Control_EnCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_BeginDate">开始日期:</label>
                <input type="text" id="Control_BeginDate" name="BeginDate"  value="@Model.BeginDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_EndDate">终止日期:</label>
                <input type="text" id="Control_EndDate" name="EndDate"  value="@Model.EndDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_GroupNumber">团队编号:</label>
                <input type="text"  value="@Model.GroupNumber" class="mini-textbox" name="GroupNumber" id="Control_GroupNumber" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_TaCode">所属旅行社:</label>
                <input type="text"  value="@Model.TaCode" class="mini-textbox" name="TaCode" id="Control_TaCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_PersonTimes">团队人数:</label>
                <input type="text" id="Control_PersonTimes" name="PersonTimes"  value="@Model.PersonTimes" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="0"/>
            </div>

            <div class="ablock">
                <label for="Control_SaleMoney">团队购物金额:</label>
                <input type="text" id="Control_SaleMoney" name="SaleMoney"  value="@Model.SaleMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_PushMoney">提成金额:</label>
                <input type="text" id="Control_PushMoney" name="PushMoney"  value="@Model.PushMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_AdjustMoney">奖励金额:</label>
                <input type="text" id="Control_AdjustMoney" name="AdjustMoney"  value="@Model.AdjustMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_TaReturnsMoney">旅行社返利金额:</label>
                <input type="text" id="Control_TaReturnsMoney" name="TaReturnsMoney"  value="@Model.TaReturnsMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_GuideCode">导游编号:</label>
                <input type="text"  value="@Model.GuideCode" class="mini-textbox" name="GuideCode" id="Control_GuideCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_ContactPhone">联系人电话:</label>
                <input type="text"  value="@Model.ContactPhone" class="mini-textbox" name="ContactPhone" id="Control_ContactPhone" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GuidePushMoney">导游提成金额:</label>
                <input type="text" id="Control_GuidePushMoney" name="GuidePushMoney"  value="@Model.GuidePushMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_Expostor">讲解员:</label>
                <input type="text"  value="@Model.Expostor" class="mini-textbox" name="Expostor" id="Control_Expostor" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_ExpostorPushMoney">讲解员提成金额:</label>
                <input type="text" id="Control_ExpostorPushMoney" name="ExpostorPushMoney"  value="@Model.ExpostorPushMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_Driver">司机:</label>
                <input type="text"  value="@Model.Driver" class="mini-textbox" name="Driver" id="Control_Driver" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_StopCharge">停车费:</label>
                <input type="text" id="Control_StopCharge" name="StopCharge"  value="@Model.StopCharge" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_NopayMoney">未结金额:</label>
                <input type="text" id="Control_NopayMoney" name="NopayMoney"  value="@Model.NopayMoney" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="2"/>
            </div>

            <div class="ablock">
                <label for="Control_RebalanceCause">补单原因:</label>
                <input type="text"  value="@Model.RebalanceCause" class="mini-textbox" name="RebalanceCause" id="Control_RebalanceCause" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_IfBalance">是否结算:</label>
                <div id="Control_IfBalance" name="IfBalance" value="1" class="mini-checkbox" checked="true" readOnly="false" text="结算"></div>
            </div>

            <div class="ablock">
                <label for="Control_CreateDate">创建时间:</label>
                <input type="text" id="Control_CreateDate" name="CreateDate"  value="@Model.CreateDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_Operator">操作员:</label>
                <input type="text"  value="@Model.Operator" class="mini-textbox" name="Operator" id="Control_Operator" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_Assessor">审核人:</label>
                <input type="text"  value="@Model.Assessor" class="mini-textbox" name="Assessor" id="Control_Assessor" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_IfExamine">是否审核:</label>
                <div id="Control_IfExamine" name="IfExamine" value="1" class="mini-checkbox" checked="true" readOnly="false" text="审核"></div>
            </div>

            <div class="ablock">
                <label for="Control_ExamineDate">审核时间:</label>
                <input type="text" id="Control_ExamineDate" name="ExamineDate"  value="@Model.ExamineDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

            <div class="ablock">
                <label for="Control_OperatorDate">操作时间:</label>
                <input type="text" id="Control_OperatorDate" name="OperatorDate"  value="@Model.OperatorDate" class="mini-datepicker"  format="yyyy-MM-dd H:mm:ss" timeFormat="H:mm:ss" showTime="true" showOkButton="true" showClearButton="true" required="false" />
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

