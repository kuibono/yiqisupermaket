@model YiQiWorkFlow.Domain.Tg.TgGroupCall
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>团队策略提醒编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "团队策略提醒"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/13 20:26:58";var id="@Model.Id";pageName="TgGroupCallEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Tg/SaveTgGroupCall",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_TgGroupCall .content").first().text(r.Message);
                        $("#Tooltip_Critical_TgGroupCall").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_TgGroupCall .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_TgGroupCall").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            团队策略提醒编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_TgGroupCall">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于团队策略提醒编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_TgGroupCall">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于团队策略提醒编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_TgGroupCall">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于团队策略提醒编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_TgGroupCall">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于团队策略提醒编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_TgFlowNumber">流水单号:</label>
                <input type="text" id="Control_TgFlowNumber" name="TgFlowNumber"  value="@Model.TgFlowNumber" class="mini-spinner" minValue="0" maxValue="1000" decimalPlaces="0"/>
            </div>

            <div class="ablock">
                <label for="Control_CallType">提醒类型:</label>
                <input type="text"  value="@Model.CallType" class="mini-textbox" name="CallType" id="Control_CallType" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_ItemsCode">编码:</label>
                <input type="text"  value="@Model.ItemsCode" class="mini-textbox" name="ItemsCode" id="Control_ItemsCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_ItemsName">名称:</label>
                <input type="text"  value="@Model.ItemsName" class="mini-textbox" name="ItemsName" id="Control_ItemsName" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_CallContent">提醒内容:</label>
                <input type="text"  value="@Model.CallContent" class="mini-textbox" name="CallContent" id="Control_CallContent" vtype="maxLength:100" />
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_Id">:</label>
                <input id="Control_Id" value="@Model.Id" name="Id" class="mini-hidden"/>
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

