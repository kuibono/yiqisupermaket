@model YiQiWorkFlow.Domain.Tg.TgTravelArchivesPushtac
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>旅行社提成策略编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "旅行社提成策略"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/15 19:58:59";var id="@Model.Id";pageName="TgTravelArchivesPushtacEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Tg/SaveTgTravelArchivesPushtac",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_TgTravelArchivesPushtac .content").first().text(r.Message);
                        $("#Tooltip_Critical_TgTravelArchivesPushtac").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_TgTravelArchivesPushtac .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_TgTravelArchivesPushtac").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            旅行社提成策略编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_TgTravelArchivesPushtac">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于旅行社提成策略编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_TgTravelArchivesPushtac">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于旅行社提成策略编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_TgTravelArchivesPushtac">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于旅行社提成策略编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_TgTravelArchivesPushtac">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于旅行社提成策略编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_TaCode">旅行社编号:</label>
                <input type="text"  value="@Model.TaCode" class="mini-textbox" name="TaCode" id="Control_TaCode" vtype="maxLength:20"required="true" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsClassCode">商品类别:</label>
                <input type="text"  value="@Model.GoodsClassCode" class="mini-textbox" name="GoodsClassCode" id="Control_GoodsClassCode" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_GoodsClassName">类别名称:</label>
                <input type="text"  value="@Model.GoodsClassName" class="mini-textbox" name="GoodsClassName" id="Control_GoodsClassName" vtype="maxLength:20" />
            </div>

            <div class="ablock">
                <label for="Control_PushRate">结算比率:</label>
                <input type="text" id="Control_PushRate" name="PushRate"  value="@Model.PushRate" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="6"/>
            </div>

            <div class="ablock">
                <label for="Control_AccountRate">挂帐比率:</label>
                <input type="text" id="Control_AccountRate" name="AccountRate"  value="@Model.AccountRate" class="mini-spinner" minValue="-999999999" maxValue="999999999" decimalPlaces="6"/>
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_Id">:</label>
                <input id="Control_Id" value="@Model.Id" name="Id" class="mini-hidden"/>
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

