@model YiQiWorkFlow.Domain.Sys.SysModule
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>功能模块设置编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "功能模块设置"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/13 20:26:57";var id="@Model.Id";pageName="SysModuleEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Sys/SaveSysModule",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_SysModule .content").first().text(r.Message);
                        $("#Tooltip_Critical_SysModule").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_SysModule .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_SysModule").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            功能模块设置编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_SysModule">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于功能模块设置编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_SysModule">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于功能模块设置编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_SysModule">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于功能模块设置编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_SysModule">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于功能模块设置编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_WindowCode">窗口代码:</label>
                <input type="text"  value="@Model.WindowCode" class="mini-textbox" name="WindowCode" id="Control_WindowCode" vtype="maxLength:50" />
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_WindowName">窗口名称:</label>
                <input id="Control_WindowName" value="@Model.Id" name="WindowName" class="mini-hidden"/>
            </div>

            <div class="ablock">
                <label for="Control_ModuleCode">模块代码:</label>
                <input type="text"  value="@Model.ModuleCode" class="mini-textbox" name="ModuleCode" id="Control_ModuleCode" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_ModuleName">模块名称:</label>
                <input type="text"  value="@Model.ModuleName" class="mini-textbox" name="ModuleName" id="Control_ModuleName" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_ModuleChild">主营业务，参数设置，查询分析:</label>
                <input type="text"  value="@Model.ModuleChild" class="mini-textbox" name="ModuleChild" id="Control_ModuleChild" vtype="maxLength:50" />
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_FunctionCode">窗口功能:</label>
                <input id="Control_FunctionCode" value="@Model.Id" name="FunctionCode" class="mini-hidden"/>
            </div>

            <div class="ablock">
                <label for="Control_OpenMode">open,opensheet:</label>
                <input type="text"  value="@Model.OpenMode" class="mini-textbox" name="OpenMode" id="Control_OpenMode" vtype="maxLength:10" />
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

