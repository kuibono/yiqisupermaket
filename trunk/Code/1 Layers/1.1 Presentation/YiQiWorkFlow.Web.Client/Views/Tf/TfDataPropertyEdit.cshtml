@model YiQiWorkFlow.Domain.Tf.TfDataProperty
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>通讯配置表字段编辑</title>
    <link type="text/css" rel="Stylesheet" href="/Styles/editPage.css" />
    <script type="text/javascript" src="/scripts/boot.js"></script>
    <script type="text/javascript" src="/Scripts/editPage.js"></script>
    <script type="text/javascript"> var pageTitle = "通讯配置表字段"; var formChanged = false; var pageType = "Edit"; var createTime = "2014/2/13 20:26:58";var id="@Model.Id";pageName="TfDataPropertyEdit"; </script>
	<script type="text/javascript">
        function SaveData() {
            var form = new mini.Form("#formMain"); // default form
            var o = form.getData();
            $.ajax({
                url: "/Tf/SaveTfDataProperty",
                type: 'post',
                data: o,
                cache: false,
				dataType:"json",
                success: function (r) {
                    if (r.IsSuccess == true) {
                        closeWithNoValidate();
                    } else {
                        $("#Tooltip_Critical_TfDataProperty .content").first().text(r.Message);
                        $("#Tooltip_Critical_TfDataProperty").show("fast");
                        form.unmask();
                        $(document).scrollTop(0);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $("#Tooltip_Critical_TfDataProperty .content").first().text(jqXHR.responseText);
                    $("#Tooltip_Critical_TfDataProperty").show("fast");
                    form.unmask();
					$(document).scrollTop(0);
                }
            });
        }
    </script>
</head>
<body>
    <div class="container">
        <h1>
            通讯配置表字段编辑
        </h1>
        <div class="tooltip warning hidden" id="Tooltip_Warning_TfDataProperty">
            <img src="/images/Warning.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于通讯配置表字段编辑页面的警告信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip info hidden" id="Tooltip_Info_TfDataProperty">
            <img src="/images/Info.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于通讯配置表字段编辑页面的提示信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip help hidden" id="Tooltip_Help_TfDataProperty">
            <img src="/images/Help.png" alt="Warning" height="48" width="48" />
			<p class="content">
            	此处填写关于通讯配置表字段编辑页面的帮助信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <div class="tooltip critical hidden" id="Tooltip_Critical_TfDataProperty">
            <img src="/images/critical.png" alt="Warning" height="48" width="48" />
			<p class="content">
            此处填写关于通讯配置表字段编辑页面的出错信息
			</p>
            <div class="button-close">
                x</div>
            <div class="buttonGroup">
                <input type="button" class="myYellowButton buttonNoMoreShow" value="不再显示" />
                <input type="button" class="myYellowButton buttonClose" value="关闭" />
            </div>
        </div>
        <form id="formMain" method="post" action="">
        <fieldset>
            <legend>基本信息</legend>
            <div class="ablock">
                <label for="Control_PropertyId">属性编号:</label>
                <input type="text"  value="@Model.PropertyId" class="mini-textbox" name="PropertyId" id="Control_PropertyId" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_PropertyObject">对象编号:</label>
                <input type="text"  value="@Model.PropertyObject" class="mini-textbox" name="PropertyObject" id="Control_PropertyObject" vtype="maxLength:50" />
            </div>

            <div class="ablock">
                <label for="Control_PropertyName">属性名称:</label>
                <input type="text"  value="@Model.PropertyName" class="mini-textbox" name="PropertyName" id="Control_PropertyName" vtype="maxLength:100" />
            </div>

            <div class="ablock">
                <label for="Control_PropertyType">属性类型:</label>
                <input type="text"  value="@Model.PropertyType" class="mini-textbox" name="PropertyType" id="Control_PropertyType" vtype="maxLength:100" />
            </div>

            <div class="ablock">
                <label for="Control_PropertyIskey">是否主键:</label>
                <div id="Control_PropertyIskey" name="PropertyIskey" class="mini-checkbox" checked="true" readOnly="false" text="是否主键"></div>
            </div>

            <div class="ablock">
                <label for="Control_PropertyIsnullable">是否为空:</label>
                <div id="Control_PropertyIsnullable" name="PropertyIsnullable" class="mini-checkbox" checked="true" readOnly="false" text="是否为空"></div>
            </div>

            <div class="ablock">
                <label for="Control_PropertyIsunqueindex">是否唯一:</label>
                <div id="Control_PropertyIsunqueindex" name="PropertyIsunqueindex" class="mini-checkbox" checked="true" readOnly="false" text="是否唯一"></div>
            </div>

            <div class="ablock">
                <label for="Control_PropertyIstransfer">是否传输:</label>
                <div id="Control_PropertyIstransfer" name="PropertyIstransfer" class="mini-checkbox" checked="true" readOnly="false" text="是否传输"></div>
            </div>

            <div class="ablock">
                <label for="Control_PropertyDescription">属性描述:</label>
                <input type="text"  value="@Model.PropertyDescription" class="mini-textbox" name="PropertyDescription" id="Control_PropertyDescription" vtype="maxLength:200" />
            </div>

            <div class="ablock">
                <label for="Control_PropertyCondition">传输条件:</label>
                <input type="text"  value="@Model.PropertyCondition" class="mini-textbox" name="PropertyCondition" id="Control_PropertyCondition" vtype="maxLength:200" />
            </div>

            <div class="ablock" style="display:none">
                <label for="Control_Id">:</label>
                <input id="Control_Id" value="@Model.Id" name="Id" class="mini-hidden"/>
            </div>

           
        </fieldset>
        <div class="clear">
        </div>
        <div class="control-panel">
            <div class="control-button">
                <input type="button" class="myButton" id="btnFormSubmit" value="保存(Ctrl+S)" />
                <input type="reset" class="myGrayButton" id="btnFormReset" value="重置(Esc)" />
                <input type="button" class="myRedButton" id="btnClosePage" value="关闭(Ctrl+Q)" />
            </div>
            <div class="botton-up">
            </div>
        </div>
        </form>
    </div>
</body>
</html>

