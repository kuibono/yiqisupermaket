<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TEWorkFlow.Domain" namespace="TEWorkFlow.Domain.Sys">
  <class name="SysPost" table="sys_Post" lazy="true" >
    <id name="Id" type="string" column="Id" >
      <generator class="assigned" />
    </id>
    <property name="Title" column="title" type="string" />
    <property name="PostTime" column="post_time" type="System.DateTime" />
    <property name="PostUser" column="post_user" type="string" />
    <property name="PostContent" column="post_content" type="string" />
  </class>
</hibernate-mapping>