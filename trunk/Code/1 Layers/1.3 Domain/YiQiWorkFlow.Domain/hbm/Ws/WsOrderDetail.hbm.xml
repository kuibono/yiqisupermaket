    <id name="Id" type="string" column="Id" >
	    <generator class="assigned" />
    </id>
	<property name="OrderNumber" column="order_number" type="string" />
	<property name="GoodsCode" column="goods_code" type="string" />
	<property name="GoodsBarCode" column="goods_bar_code" type="string" />
	<property name="Specification" column="specification" type="string" />
	<property name="PackUnitCode" column="pack_unit_code" type="string" />
	<property name="PackQty" column="pack_qty" type="decimal" />
	<property name="PackCoef" column="pack_coef" type="decimal" />
	<property name="ReturnQty" column="return_qty" type="decimal" />
	<property name="TradePrice" column="trade_price" type="decimal" />
	<property name="SaleMoney" column="sale_money" type="decimal" />
	<property name="DiscountMoney" column="discount_money" type="decimal" />
  </class>
