  <class name="MsExchangeDetail" table="ms_exchange_detail" lazy="true" >
    <id name="Id" type="string" column="Id" >
	    <generator class="assigned" />
    </id>
	<property name="ExchangeNumber" column="exchange_number" type="string" />
	<property name="sNumber" column="s_number" type="decimal" />
	<property name="GoodsCode" column="goods_code" type="string" />
	<property name="GoodsName" column="goods_name" type="string" />
	<property name="ExchangeQty" column="exchange_qty" type="decimal" />
	<property name="ExchangePoints" column="exchange_points" type="decimal" />
	<property name="PointsTotal" column="points_total" type="decimal" />
	<property name="GrossMoney" column="gross_money" type="decimal" />
	<property name="NontaxGrossMoney" column="nontax_gross_money" type="decimal" />
	<property name="SysGuid" column="sys_guid" type="string" />
  </class>
</hibernate-mapping>

